<% provide(:title, "Log In")%>

<h1>Go ahead and log in</h1>

<%= form_for(:session, url: login_path) do |f| %>

  <%= f.label :username, "Username: " %>
  <%= f.text_field :username %>

  <%= f.label :password, "Password: " %>
  <%= f.password_field :password %>

  <%= f.submit "Log In" %>

<% end %>